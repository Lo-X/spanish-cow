{
    "name": "lo-x/spanish-cow",
    "description": "Localization interface and API to translate your projects and synchronize translations",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Lo√Øc Boutter",
            "email": "loic.boutter@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-iconv": "*",
        "api-platform/api-pack": "^1.1",
        "enqueue/enqueue-bundle": "^0.8.31",
        "enqueue/fs": "^0.8.24",
        "incenteev/composer-parameter-handler": "^2.1",
        "knplabs/doctrine-behaviors": "^1.5",
        "lexik/jwt-authentication-bundle": "^2.4",
        "php-translation/symfony-bundle": "^0.7.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/filesystem": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.0",
        "white-october/pagerfanta-bundle": "^1.2",
        "whiteoctober/breadcrumbs-bundle": "^1.4"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fzaninotto/faker": "^1.7",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^4.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "platform": {
            "php": "7.1.3"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "incenteev-parameters": {
            "file": "config/parameters.yaml"
        },
        "symfony": {
            "id": "01CA608ADS8X2E4VHJ4ENF6R6Z",
            "allow-contrib": false
        }
    }
}
